<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<bookmarks>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.pi_flexform.bookmarks</label>
							<config>
								<type>select</type>
								<size>20</size>
								<maxitems>1000</maxitems>
								<itemsProcFunc>tx_sexybookmarks_itemsProcFunc->getFollows</itemsProcFunc>
							</config>
						</TCEforms>
					</bookmarks>
				</el>
			</ROOT>
		</general>
		<settings>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.pi_flexform.sheet_settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<bookmarkCenter>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.tx_sexybookmarks.bookmarkCenter</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</bookmarkCenter>
					<bookmarkExpandable>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.tx_sexybookmarks.bookmarkExpandable</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</bookmarkExpandable>
					<bookmarkBackground>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.tx_sexybookmarks.style</label>
							<config>
								<type>select</type>
								<size>1</size>
								<maxitems>1</maxitems>
								<itemsProcFunc>tx_sexybookmarks_itemsProcFunc->getBackgroundClasses</itemsProcFunc>
							</config>
						</TCEforms>
					</bookmarkBackground>
				</el>
			</ROOT>
		</settings>
		<movement>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.pi_flexform.sheet_movement</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<transition>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.pi_flexform.transition</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">Quad</numIndex>
										<numIndex index="1">Quad</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Cubic</numIndex>
										<numIndex index="1">Cubic</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">Quart</numIndex>
										<numIndex index="1">Quart</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">Quint</numIndex>
										<numIndex index="1">Quint</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">Expo</numIndex>
										<numIndex index="1">Expo</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">Circ</numIndex>
										<numIndex index="1">Circ</numIndex>
									</numIndex>
									<numIndex index="7" type="array">
										<numIndex index="0">Sine</numIndex>
										<numIndex index="1">Sine</numIndex>
									</numIndex>
									<numIndex index="8" type="array">
										<numIndex index="0">Back</numIndex>
										<numIndex index="1">Back</numIndex>
									</numIndex>
									<numIndex index="9" type="array">
										<numIndex index="0">Bounce</numIndex>
										<numIndex index="1">Bounce</numIndex>
									</numIndex>
									<numIndex index="10" type="array">
										<numIndex index="0">Elastic</numIndex>
										<numIndex index="1">Elastic</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</transition>
					<transitionDir>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.pi_flexform.transitionDir</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">In</numIndex>
										<numIndex index="1">In</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Out</numIndex>
										<numIndex index="1">Out</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">InOut</numIndex>
										<numIndex index="1">InOut</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</transitionDir>
					<transitionDuration>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:sexybookmarks/locallang_db.xml:tt_content.pi_flexform.transitionDuration</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</transitionDuration>
				</el>
			</ROOT>
		</movement>
	</sheets>
</T3DataStructure>
